  "license": "EPL",
  "bugs": {
    "url": "https://github.com/glenjamin/checkers/issues"
  },
  "homepage": "https://github.com/glenjamin/checkers",
  "devDependencies": {
    "lodash": "^2.4.1",
    "mocha": "^2.1.0",
    "source-map-support": "^0.2.9"
  }
}
