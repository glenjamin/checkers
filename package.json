{
  "name": "checkers",
  "version": "0.10.0",
  "description": "Property-based testing for JavaScript via ClojureScript's test.check",
  "main": "checkers.js",
  "scripts": {
    "clean": "lein clean",
    "build": "lein cljsbuild once release",
