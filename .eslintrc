{
    "env": {
        "node": true
    },
    "rules": {
        "strict": true,
        "quotes": false,
        "no-use-before-define": "func",
        "no-unused-vars": [2, "all"],
        "no-mixed-requires": [1, true],
        "max-depth": [1, 5],
        "max-len": [1, 80, 4],
        "eqeqeq": false,
        "no-path-concat": false,
        "no-else-return": true,
        "no-eq-null": true,
        "no-lonely-if": true
    }
}
